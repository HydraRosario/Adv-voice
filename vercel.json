{
    "version": 2,
    "functions": {
        "api/*.py": {
            "runtime": "python3.9",
            "memory": 1024,
            "maxDuration": 10
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/app.py"
        }
    ],
    "env": {
        "PYTHONPATH": ".",
        "FLASK_ENV": "production"
    },
    "public": true
}