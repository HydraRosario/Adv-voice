{
    "version": 2,
    "builds": [
        {
            "src": "wsgi.py",
            "use": "@vercel/python"
        },
        {
            "src": "static/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1",
            "headers": {
                "cache-control": "no-cache, no-store, must-revalidate"
            }
        },
        {
            "src": "/(.*)",
            "dest": "/wsgi.py"
        }
    ]
}
